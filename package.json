	"main": "index.js",
	"scripts": {
		"lint": "eslint .",
		"test": "mocha",
		"start": "docker-compose up -d && webpack --watch",
		"stop": "docker-compose down",
		"build:server": "webpack",
		"build:clients": "webpack --config assets/webpack.config.js",
		"build": "concurrently \"npm run build:server\" \"npm run build:clients\"",
		"dev:server": "cross-env NODE_ENV=development webpack --watch",
		"dev:clients": "cross-env NODE_ENV=development webpack-dev-server --config assets/webpack.config.js",
		"dev": "concurrently \"npm run dev:server\" \"npm run dev:clients\""
	},
